	</parameter>
	<parameter name="DateAcct1" class="java.sql.Timestamp">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="DateAcct2" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="C_BPartner_ID" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
	AD_Client_ID,
	AD_Org_ID,
	C_BPartner_ID,
	Name,
	Date,
	IsNatural,
	Legal,
	Venezuelan,
	IsForeign,
	Resident,
	NoResident,
	Constituted,
	NoConstituted,
	Cedula,
	Passports,
	RFC,
	Month,
	Value,
	C_Location_ID,
	LastDayOfMonth,
	Year,
	C_Invoice_ID,
	DateAcct,
	DateInvoiced,
	IsSoTrx,
	TotalLines,
	Rif,
	DocumentNo,
	DocStatus,
	DocBaseType,
	Rate,
	ConceptoRetencion,
	Aliquot,
	MinValue,
	SubTrahend,
	MuestAnulado,
	DocRelacionado,
	Nota_Debito,
	Nota_Credito,
	Nomb_Socio,
	TotalPAgado,
	Sum(TotalBI) AS TotalBI,
	Sum(MontoRetenido) * -1 AS MontoRetenido,
	SocioNegocio,
	RIF_Libro,
	C_DocType_ID,
	Location,
	Aliquot::Varchar || ' %' AS Alicuota_Libro
 FROM
(

SELECT
	bp.ad_client_id,
	bp.ad_org_id,
	bp.c_bpartner_id,
	bp.name,
	Extract("Month" FROM i.dateacct)||'-'||EXTRACT("Year" FROM i.dateacct) As Date,
	tp.Name AS TypePerson,
	bp.TaxID,
     	CASE
            WHEN  tp.Value = 'N'
            THEN '[X]'::text
            ELSE '[ ]'::text
        END AS IsNatural,

        CASE
            WHEN  tp.Value = 'J'
            THEN '[X]'::text
            ELSE '[ ]'::text
        END AS Legal,

        CASE
            WHEN  substring(bp.TaxID from 0 for 2)  = 'V'
            THEN '[X]'::text
            ELSE '[ ]'::text
        END AS Venezuelan,
        CASE
            WHEN  substring(bp.TaxID from 0 for 2)  = 'E'  THEN '[X]'::text
            ELSE '[ ]'::text
        END AS IsForeign,
	CASE
            WHEN tp.Value = 'N'::bpchar AND substring(bp.TaxID from 0 for 2) = 'E'::bpchar
		THEN '[X]'::text
            ELSE '[ ]'::text
        END AS Resident,
        '[ ]'::text AS NoResident,
        CASE
            WHEN tp.Value = 'J'::bpchar AND substring(bp.TaxID from 0 for 2) = 'E'::bpchar
		THEN '[X]'::text
		ELSE '[ ]'::text
	END AS Constituted,
	'[ ]'::text AS NoConstituted,

        CASE
            WHEN tp.Value = 'N'::bpchar AND ( substring(bp.TaxID from 0 for 2) = ANY (ARRAY['V'::bpchar, 'E'::bpchar]))
		THEN bp.value
            ELSE '  '::character varying
        END AS Cedula,
        ''::character varying(30) AS Passports,
        CASE
            WHEN tp.Value = 'J'::bpchar
            THEN bp.value
            ELSE ''::character varying
        END AS RFC,
	bp.value,
	bpl.c_location_id,
	date_part('day'::text, to_date((('01'::text ||
        CASE
            WHEN (date_part('month'::text, i.dateacct) + 1::double precision) < 10::double precision THEN '0'::text
            ELSE ''::text
        END) || (date_part('month'::text, i.dateacct) + 1::double precision)) || date_part('year'::text, i.dateacct), 'DDMMYYYY'::text) - 1)::character varying AS lastdayofmonth, (
        CASE
            WHEN date_part('month'::text,i.dateacct) < 10::double precision THEN '0'::text
            ELSE ''::text
        END || date_part('month'::text, i.dateacct))::character varying AS Month,
	date_part('year'::text,i.dateacct)::character varying AS Year,
	i.c_invoice_id,
	i.dateacct,
	i.dateinvoiced,
	i.issotrx,
	i.totallines,
	i.LVE_WH_Concept_ID,
	btrim(bp.value::text) AS rif,
        CASE
            WHEN i.c_invoice_id IS NULL THEN i.documentno
            ELSE NULL::character varying
        END AS documentno,
        i.docstatus,
	dt.docbasetype,
	ifac.Rate,
	wc.name AS conceptoretencion,
	whc.aliquot,
	whc.value AS WithholdingCombination,
	wcf.MinValue,
	wcf.Subtrahend,
        CASE
            WHEN i.docstatus = 'RE'::bpchar THEN 'Anulado'::text
            ELSE ''::text
        END AS muestanulado,
        iaffected.documentno AS docrelacionado,
        CASE
            WHEN i.c_invoice_id IS NOT NULL AND dt.docbasetype <> ALL (ARRAY['ARC'::bpchar, 'APC'::bpchar]) THEN i.documentno
            ELSE NULL::character varying
        END AS nota_debito,
        CASE
            WHEN i.c_invoice_id IS NOT NULL AND dt.docbasetype = ANY (ARRAY['ARC'::bpchar, 'APC'::bpchar]) THEN i.documentno
            ELSE NULL::character varying
        END AS nota_credito,

	bp.name AS nomb_socio,
	i.totallines AS totalpagado,
	CASE
            WHEN
		NOT EXISTS(
			SELECT 1
			FROM LVE_Withholding w
			WHERE w.C_Charge_ID = il.C_Charge_ID) THEN
			il.linenetamt
            ELSE 0
        END AS totalbi,

	CASE
            WHEN
		NOT EXISTS(
			SELECT 1
			FROM LVE_Withholding w
			WHERE w.C_Charge_ID = il.C_Charge_ID) THEN
			il.linenetamt
            ELSE 0
        END AS montoretenido,

        CASE
            WHEN i.docstatus = 'RE'::bpchar THEN 'Anulado'::character varying
            ELSE bp.name
        END AS socionegocio,
        CASE
            WHEN i.docstatus = 'RE'::bpchar THEN ''::text
            ELSE btrim(bp.value::text)
        END AS rif_libro,
        to_char(ifac.Rate, '99'::text) || ' %'::text AS alicuota_libro,
        i.c_doctype_id,
	CASE
		WHEN coalesce(cl.address1,'')||' '||coalesce(cl.address2,'') ||', '||coalesce(cl.city,'') <> ''
		THEN coalesce(cl.address1,'')||' '||coalesce(cl.address2,'') ||', '||coalesce(cl.city,'')
		ELSE ''
	END as Location
FROM C_Invoice i
JOIN C_DocType dt ON i.C_DocType_ID = dt.C_DocType_ID
JOIN C_InvoiceLine il ON il.C_Invoice_ID = i.C_Invoice_ID
JOIN C_BPartner bp ON bp.C_BPartner_ID = i.C_BPartner_ID
LEFT JOIN LVE_PersonType tp ON bp.LVE_PersonType_id = tp.LVE_PersonType_id
LEFT JOIN C_BPartner_Location bpl ON bp.C_BPartner_ID = bpl.C_BPartner_ID
JOIN C_Location cl ON bpl.C_Location_ID = cl.C_Location_ID

 JOIN (
	SELECT
		it.C_Invoice_ID,
		SUM(
			CASE WHEN it.TaxAmt = 0::NUMERIC THEN
				it.TaxBaseAmt
			ELSE 0::numeric
			END
		)AS TotalEx,
		SUM(
			CASE WHEN it.TaxAmt <> 0::NUMERIC THEN
				it.TaxBaseAmt
			ELSE 0::numeric
			END
		)AS TotalBi,
		SUM(
			CASE WHEN it.TaxAmt <> 0::NUMERIC THEN
				it.TaxAmt
			ELSE 0::numeric
			END
		)AS TotalImpuesto,
		MAX(t.Rate) AS Rate
	FROM C_InvoiceTax it
	JOIN C_Tax t ON t.C_Tax_ID = it.C_Tax_ID
	GROUP BY
		it.C_Invoice_ID
) ifac ON ifac.C_Invoice_ID = i.C_Invoice_ID
LEFT JOIN C_Invoice iaffected ON iaffected.C_Invoice_ID = i.C_Invoice_ID
JOIN LVE_WH_Concept wc ON wc.LVE_WH_Concept_ID = i.LVE_WH_Concept_ID
LEFT JOIN LVE_WH_Combination whc ON whc.LVE_PersonType_ID = bp.LVE_PersonType_ID AND whc.LVE_WH_Concept_ID = i.LVE_WH_Concept_ID
JOIN LVE_WH_Config wcf ON wcf.LVE_WH_Combination_ID = whc.LVE_WH_Combination_ID
WHERE
	i.DocStatus = ANY (ARRAY['CO'::bpchar, 'CL'::bpchar])  AND
	i.IsSotrx = 'N'::bpchar AND
	bpl.IsBillTo = 'Y'::bpchar AND
	i.DateAcct BETWEEN $P{DateAcct1} AND $P{DateAcct2} AND
	(i.C_BPartner_ID = $P{C_BPartner_ID} OR $P{C_BPartner_ID} IS NULL)AND

	EXISTS(
		SELECT 1
		FROM C_Invoiceline il
		JOIN LVE_Withholding w ON w.C_Charge_ID = il.C_Charge_ID
	)AND
	 i.LVE_WH_Concept_ID <> 0
  GROUP BY

	i.c_bpartner_id,i.c_invoice_id, i.ad_client_id, i.ad_org_id, i.c_bpartner_id, i.dateacct, i.dateinvoiced, i.grandtotal, i.issotrx, i.totallines,
	i.LVE_WH_Concept_ID, bp.Value, i.documentno, i.docstatus, ifac.totalex, ifac.totalbi, ifac.totalimpuesto, bp.name, iaffected.documentno, dt.issotrx,
	i.c_invoice_id, dt.docbasetype, ifac.Rate,
	wc.name, whc.aliquot,whc.value,	wcf.Subtrahend,  i.c_doctype_id,wcf.MinValue,
	bp.ad_client_id, bp.ad_org_id,
	bp.c_bpartner_id,
	bp.name,tp.Value,bpl.c_location_id,il.C_Charge_ID,il.linenetamt,Location,tp.Name
ORDER BY
	bp.c_bpartner_id
)
as temp

Group By
	AD_Client_ID,
	AD_Org_ID,
	C_BPartner_ID,
	Name,
	Date,
	IsNatural,
	Legal,
	Venezuelan,
	IsForeign,
	Resident,
	NoResident,
	Constituted,
	NoConstituted,
	Cedula,
	Passports,
	RFC,
	Value,
	C_Location_ID,
	LastDayOfMonth,
	Year,
	C_Invoice_ID,
	DateAcct,
	DateInvoiced,
	IsSoTrx,
	TotalLines,
	Rif,
	DocumentNo,
	DocStatus,
	DocBaseType,
	Rate,
	ConceptoRetencion,
	Aliquot,
	MinValue,
	SubTrahend,
	MuestAnulado,
	DocRelacionado,
	Nota_Debito,
	Nota_Credito,
	Nomb_Socio,
	TotalPAgado,
	SocioNegocio,
	RIF_Libro,
	Alicuota_Libro,
	C_DocType_ID,
	Location,
	Month
ORDER BY
	C_BPartner_ID,DateAcct]]>
	</queryString>
